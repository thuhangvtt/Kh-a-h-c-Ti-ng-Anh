-- Xóa database nếu đã tồn tại
DROP DATABASE IF EXISTS KHTA;

-- Tạo database mới
CREATE DATABASE KHTA;
USE KHTA;

-- Bảng Users
CREATE TABLE Users (
    user_id INT AUTO_INCREMENT PRIMARY KEY,
    user_name VARCHAR(255),
    name VARCHAR(255) NOT NULL,
    email VARCHAR(255) NOT NULL UNIQUE,
    password VARCHAR(255) NOT NULL,
    telephone VARCHAR(15),
    disabled BIT DEFAULT 0,
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    modified_at DATETIME ON UPDATE CURRENT_TIMESTAMP 
);

-- Bảng admin_account
CREATE TABLE admin_account (
    user_id INT AUTO_INCREMENT PRIMARY KEY,
    username VARCHAR(255) NOT NULL UNIQUE,
    email VARCHAR(255) NOT NULL UNIQUE,
    password VARCHAR(255) NOT NULL,
    name VARCHAR(255),
    telephone VARCHAR(15),
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    modified_at DATETIME ON UPDATE CURRENT_TIMESTAMP 
);

-- Bảng categories
CREATE TABLE categories (
    id INT AUTO_INCREMENT PRIMARY KEY,
    slug VARCHAR(255),
    img VARCHAR(255),
    name VARCHAR(255) NOT NULL UNIQUE,
    description TEXT
);

-- Bảng courses
CREATE TABLE courses (
    id INT AUTO_INCREMENT PRIMARY KEY,
    name VARCHAR(500) NOT NULL UNIQUE,
    slug VARCHAR(500) NOT NULL,
    image VARCHAR(255),
    description TEXT,
    category_id INT NOT NULL,
    NumberEnrolled INT,
    Objectives TEXT,
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    modified_at DATETIME ON UPDATE CURRENT_TIMESTAMP,
    approve_at DATE DEFAULT NULL,
    disabled BIT DEFAULT 0,
    FOREIGN KEY (category_id) REFERENCES categories(id) ON DELETE CASCADE
);

-- Bảng Lessons
CREATE TABLE Lessons (
    id INT AUTO_INCREMENT PRIMARY KEY,
    course_id INT NOT NULL,
    title VARCHAR(500) NOT NULL,
    slug VARCHAR(500),
    video_link TEXT,
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    is_disabled BIT DEFAULT 0,
    FOREIGN KEY (course_id) REFERENCES courses(id) ON DELETE CASCADE
);

-- Bảng enrollments
CREATE TABLE enrollments (
    user_id INT NOT NULL,
    course_id INT NOT NULL,
    enrolled_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    status VARCHAR (50) DEFAULT 'pending',
    PRIMARY KEY (user_id, course_id),
    FOREIGN KEY (user_id) REFERENCES Users(user_id) ON DELETE CASCADE,
    FOREIGN KEY (course_id) REFERENCES courses(id) ON DELETE CASCADE
);

-- Bảng Exercises
CREATE TABLE Exercises (
    exercise_id INT AUTO_INCREMENT PRIMARY KEY,
    lesson_id INT NOT NULL,
    exercise_name VARCHAR(100) NOT NULL,
    exercise_link TEXT NOT NULL,
    FOREIGN KEY (lesson_id) REFERENCES Lessons(id) ON DELETE CASCADE
);

-- Bảng Progress
CREATE TABLE Progress (
    progress_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT NOT NULL,
    course_id INT NOT NULL,
    lessons_completed INT DEFAULT 0,
    exercises_completed INT DEFAULT 0,
    last_accessed_date DATE,
    FOREIGN KEY (user_id) REFERENCES Users(user_id) ON DELETE CASCADE,
    FOREIGN KEY (course_id) REFERENCES courses(id) ON DELETE CASCADE
);

-- Bảng Notifications
CREATE TABLE Notifications (
    notification_id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT NOT NULL,
    message TEXT,
    notification_date DATETIME DEFAULT CURRENT_TIMESTAMP,
    status VARCHAR(10) CHECK (status IN ('unread', 'read')) DEFAULT 'unread',
    FOREIGN KEY (user_id) REFERENCES Users(user_id) ON DELETE CASCADE
);

-- Bảng StudyPaths
CREATE TABLE StudyPaths (
    path_id INT AUTO_INCREMENT PRIMARY KEY,
    course_id INT NOT NULL,
    path_name VARCHAR(100),
    lesson_id INT,
    FOREIGN KEY (course_id) REFERENCES courses(id) ON DELETE CASCADE,
    FOREIGN KEY (lesson_id) REFERENCES Lessons(id) ON DELETE SET NULL
);

-- Bảng VocabularyLists
CREATE TABLE VocabularyLists (
    vocab_list_id INT AUTO_INCREMENT PRIMARY KEY,
    list_name VARCHAR(100) NOT NULL,
    user_id INT NOT NULL,
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES Users(user_id) ON DELETE CASCADE
);

-- Bảng Vocabulary
CREATE TABLE Vocabulary (
    vocab_id INT AUTO_INCREMENT PRIMARY KEY,
    vocab_list_id INT NOT NULL,
    word VARCHAR(100) NOT NULL,
    definition TEXT,
    FOREIGN KEY (vocab_list_id) REFERENCES VocabularyLists(vocab_list_id) ON DELETE CASCADE
);

-- Bảng Ratings
CREATE TABLE Ratings (
    id INT AUTO_INCREMENT PRIMARY KEY,
    user_id INT NOT NULL,
    course_id INT NOT NULL,
    review_text TEXT,
    created_at DATETIME DEFAULT CURRENT_TIMESTAMP,
    FOREIGN KEY (user_id) REFERENCES Users(user_id) ON DELETE CASCADE,
    FOREIGN KEY (course_id) REFERENCES courses(id) ON DELETE CASCADE
);

